{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": [true, "ban-single-arg-parens"],
      "comment-format": [true, {"ignore-words": ["TODO"]}],
      "interface-name": false,
      "no-console": false,
      "object-literal-sort-keys": false,
      "quotemark": [true, "single", "jsx-double"],
      "trailing-comma": false
    },
    "rulesDirectory": []
}
