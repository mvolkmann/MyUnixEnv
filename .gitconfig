[url "https://"]
  insteadOf = git://
[alias]
  br = branch
  ci = commit
  co = checkout
  lastchange = log -p --follow -n 1
[core]
  editor = /usr/local/bin/vim
[user]
  name = R. Mark Volkmann
  email = r.mark.volkmann@gmail.com
[color]
  ui = auto
[credential]
  helper = osxkeychain
[push]
  default = simple
[http]
	sslVerify = false
